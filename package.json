{
  "name": "com.karinacortes",
  "displayName": "LIN Karina Cortes",
  "version": "1.1.0",
  "description": "My personal web site",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf www/js/bundle.js",
    "build-dev": "npm run clean && SET NODE_ENV=develop & webpack --config webpack.config.js",
    "build-prod": "npm run clean && SET NODE_ENV=production & webpack --config webpack.config.prod.js",
    "start": "./node_modules/.bin/webpack-dev-server",
    "lint": "eslint src",
    "lint-report": "./node_modules/.bin/eslint -f html src > lint-report.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carloshglez/website.git"
  },
  "author": "Carlos Gonzalez",
  "license": "ISC",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-icons": "^2.2.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "rimraf": "^2.6.3",
    "source-map": "^0.7.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
